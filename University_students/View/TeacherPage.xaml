<UserControl x:Class="University_students.View.TeacherPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:University_students.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="2,0,-2,0" Background="#FFAAB4F7">
        <TabControl>
            <TabItem Header="Groups">
                <Grid Background="#FFE5E5E5" Margin="0,-1,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="206*"/>
                        <ColumnDefinition Width="191*"/>
                    </Grid.ColumnDefinitions>
                </Grid>
            </TabItem>
            <TabItem Header="My Subjects">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Reports">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem Header="Exams">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
        </TabControl>
        <Button  Command="{Binding LogOutCommand}"  Content="Log Out" Height="Auto" Margin="704,0,0,427" Background="#FFF51C12" BorderBrush="#FFF34605" FontSize="11"/>
    </Grid>
</UserControl>
